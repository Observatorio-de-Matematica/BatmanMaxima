/* [wxMaxima: title   start ]
Definição
   [wxMaxima: title   end   ] */

/* [wxMaxima: fold    start ] */

/* [wxMaxima: input   start ] */
n(x):=x^2+1;
d(x):=x+1;
/* [wxMaxima: input   end   ] */

/* [wxMaxima: input   start ] */
f(x):=n(x)/d(x);
f(x);
factor(f(x));
/* [wxMaxima: input   end   ] */

/* [wxMaxima: input   start ] */
f(x)=0;
/* [wxMaxima: input   end   ] */

/* [wxMaxima: input   start ] */
solve(f(x)=0);
/* [wxMaxima: input   end   ] */

/* [wxMaxima: fold    end   ] */

/* [wxMaxima: title   start ]
Assíntotas
   [wxMaxima: title   end   ] */

/* [wxMaxima: fold    start ] */

/* [wxMaxima: comment start ]
       n(x)
f(x) = ----
       d(x)

n(x) = d(x)q(x) + r(x), grau(r) < grau(d)

              r(x)
f(x) = q(x) + ----
              d(x)

        2      2          2
       x +1   x +1+1-1   x -1+2   (x+1)(x-1)+2          2
f(x) = ---- = -------- = ------ = ------------ = x-1 + ---
        x+1      x+1       x+1        x+1              x+1
   [wxMaxima: comment end   ] */

/* [wxMaxima: input   start ] */
assint1_:[parametric,-1,t,[t,-8,8]];
assint2_:x-1;
/* [wxMaxima: input   end   ] */

/* [wxMaxima: fold    end   ] */

