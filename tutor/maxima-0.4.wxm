/* [wxMaxima: input   start ] */
n(x,a):=x^4-a*x^2;
d(x  ):=x^3-1;
f(x,a):=n(x,a)/d(x);
f(x,a);
with_slider
(
  a, makelist(i/2, i, -60, 60),
  [f(x,a)],
  [x, -20, 20],
  [y, -30, 30],
  [box,false],
  [gnuplot_preamble, "set size ratio 1"]
),wxplot_size=[800,800];
/* [wxMaxima: input   end   ] */

/* [wxMaxima: input   start ] */
with_slider
(
  n, makelist(i/10, i, 0, 20),
  [sin(x)+n*x],
  [x, -4*%pi, 4*%pi],
  [y, -4*%pi, 4*%pi],
  [box,false],
  [gnuplot_preamble, "set size ratio 1"]
);
