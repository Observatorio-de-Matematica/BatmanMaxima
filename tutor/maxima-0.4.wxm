d1(x);
factor(d1(x));
/* [wxMaxima: input   end   ] */

/* [wxMaxima: input   start ] */
d1(x)=0;
/* [wxMaxima: input   end   ] */

/* [wxMaxima: input   start ] */
solve(d1(x)=0);
/* [wxMaxima: input   end   ] */

/* [wxMaxima: fold    end   ] */

